node {
  stage('Preparation') {
    git 'https://github.com/devopsjourney1/jenkins-101.git'
  }
  stage('Test') {
      git 'https://github.com/devopsjourney1/jenkins-101.git'
      dir('myapp') {
        bat 'pip install -r requirements.txt'
        bat 'python3 hello.py --name=Adithya'
      }
  }
    stage('Deploy') {
      git 'https://github.com/devopsjourney1/jenkins-101.git'
      dir('myapp') {
